{
  "name": "@repo/date-picker",
  "version": "1.0.6",
  "description": "React DatePicker component with dayjs integration",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/date-picker.css",
    "./style.css": "./dist/date-picker.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "react",
    "datepicker",
    "date-picker",
    "dayjs",
    "react-datepicker",
    "date",
    "calendar"
  ],
  "author": "KIM",
  "license": "MIT",
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .turbo node_modules",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "dependencies": {
    "date-fns": "catalog:",
    "dayjs": "catalog:",
    "lodash-es": "catalog:",
    "react-datepicker": "catalog:"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/lodash-es": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react-swc": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:"
  }
}
